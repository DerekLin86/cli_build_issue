{
    "compilerOptions": {
        "baseUrl": ".",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "sourceMap": true,
        "suppressImplicitAnyIndexErrors": true,
        "target": "es5",
        "lib": [
            "es2015",
            "dom"
        ],
        "paths": {
            "rxjs/*": [
                "source_pool/rxjs/*"
            ]
        }
    },
    "exclude": [
        "source_pool/*"
    ]
}
